{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "context",
  "title": "_Context",
  "description": "Context Hijacking app's Command Pallets",
  "icon": "command-icon.png",
  "author": "ar4s-eth",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "_Context",
      "subtitle": "Search",
      "description": "Context Matters",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.30.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
    
  }
}